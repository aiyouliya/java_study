package com.yezi.thread;

public class MyThread extends Thread{
	private String name;
	public MyThread	(String name) {
		this.name =  name;
	}
	public void run() {
		for (int  = 0; i < 10; i++) {
			System.out.println(name + ": ");
		}
		
	}
}

public class MyThread {

	public static void main(String[] args) {

	}

}
