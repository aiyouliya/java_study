package com.yezi.mapdemo;

import java.util.HashMap;
import java.util.Map;

public class Mapdemo01 {

	public static void main(String[] args) {
		Map<String, String> map = new HashMap<>();
		map.put("name", "jamy");	//通过put()方法向map表中添加数据
		map.put("age", "16");
		map.put("hight", "178");
		map.put("weight", "68");
		map.put("EDU", "MID_school");
		map.put("next_edu", "High_school\n");
		if (map.containsKey("name")) {	//通过判断键是否存在
			System.out.println(map);
		} else {
			System.out.println("name is not content in the map!s");
		}
		if (map.containsValue("jamy")) {
			System.out.println("jamy is right!");
		} else {
			System.out.println("there is no value in the map!");

		}
	}

}
