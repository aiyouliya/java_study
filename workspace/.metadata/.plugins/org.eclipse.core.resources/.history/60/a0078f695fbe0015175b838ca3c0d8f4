package com.yezi.test;

public class UiTest {

	public static void main(String[] args) {
//		System.out.println("hello yezi test");
//		say();
		add(10.1,20.2);
	}
	private static void add(double d, double e) {
		// TODO Auto-generated method stub
		System.out.println(d+e);
	}
	public void startTest(){
		System.out.print("case pass");
	}
	public static int say(){
		System.out.println("a add b is a + b !");
		return 0;
	}
	public void tell(int a, String b){
		String b1 = "0";
		b1= a + b ;
		System.out.println(b1);
	}
	

	//reload
	public static void add(int i, int j){
		System.out.println(i+j);
	}
	public static void add(float i, float j){
		System.out.println(i+j);
	}
}
