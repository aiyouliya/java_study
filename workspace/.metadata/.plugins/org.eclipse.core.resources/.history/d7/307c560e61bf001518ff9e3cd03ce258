/*
 * 接口中应用 泛型
 */
interface GinInter<T>{
	public void say();
}
class Gin<T> implements GinInter<T>{
	private String info;
	public Gin(String info) {
		this.info = info;
	}
	public String getInfo() {
		return info;
	}
	public void setInfo(String info) {
		this.info = info;
	}
	@Override
	public void say() {
		Gin<String> g = new Gin<String>("叶子");
		System.out.println(g.getInfo());
	}
	
}
public class GenericDemon05 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
